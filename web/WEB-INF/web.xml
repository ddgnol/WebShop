<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>deleteProFromCart</servlet-name>
        <servlet-class>Controller.DeleteProInCart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>searchPro</servlet-name>
        <servlet-class>Controller.searchPro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdatePro</servlet-name>
        <servlet-class>Controller.UpdatePro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateCart</servlet-name>
        <servlet-class>Controller.UpdateCart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckOut</servlet-name>
        <servlet-class>Controller.CheckOut</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>deleteProFromCart</servlet-name>
        <url-pattern>/deleteProFromCart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>searchPro</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdatePro</servlet-name>
        <url-pattern>/UpdatePro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateCart</servlet-name>
        <url-pattern>/UpdateCart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckOut</servlet-name>
        <url-pattern>/CheckOut</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
